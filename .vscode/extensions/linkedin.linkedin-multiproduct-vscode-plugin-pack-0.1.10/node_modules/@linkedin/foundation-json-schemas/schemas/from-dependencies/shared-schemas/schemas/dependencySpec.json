{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "name": {
      "id": "li-json-schema://name",
      "type": "string"
    },
    "group": {
      "id": "li-json-schema://group",
      "type": "string"
    },
    "version": {
      "id": "li-json-schema://version",
      "type": "string"
    },
    "dependencySpecVersion": {
      "id": "li-json-schema://dependencySpecVersion",
      "type": "string",
      "default": "2.0.0"
    },
    "scmUrl": {
      "id": "li-json-schema://scmUrl",
      "type": "string"
    },
    "owners": {
      "id": "li-json-schema://owners",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "service_guard_owners": {
      "id": "li-json-schema://service_guard_owners",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "runtime": {
      "type": "object",
      "additionalProperties": true
    },
    "codecoveragecheckinprecommit": {
      "type": "object",
      "additionalProperties": true
    },
    "workspace_siblings": {
      "id": "li-json-schema://workspace_siblings",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "type": {
      "id": "li-json-schema://type",
      "type": "string",
      "enum": [
        "legacy"
      ]
    },
    "comment": {
      "id": "li-json-schema://comment",
      "type": "string"
    },
    "description": {
      "id": "li-json-schema://description",
      "type": "string"
    },
    "watchers": {
      "id": "li-json-schema://watchers",
      "type": "array",
      "items": [
        {
          "type": "string"
        }
      ]
    },
    "build": {
      "$ref": "build.json"
    },
    "review": {
      "$ref": "review.json"
    },
    "topology": {
      "$ref": "topology.json"
    },
    "archive": {
      "$ref": "archive.json"
    },
    "dependencyResolution": {
      "$ref": "dependencyResolution.json"
    },
    "external": {
      "$ref": "external.json"
    },
    "product": {
      "type": "object",
      "additionalProperties": {
        "$ref": "resolvedProductDependency.json"
      }
    },
    "trunkDev": {
      "$ref": "trunkDev.json"
    },
    "extSCM": {
      "$ref": "extSCM.json"
    },
    "variants": {
      "$ref": "variants.json"
    },
    "variant_configurations": {
      "type": "object",
      "additionalProperties": {
        "type": "object"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "name",
    "group",
    "version",
    "scmUrl",
    "build",
    "external",
    "product"
  ]
}
