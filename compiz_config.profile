[freewins]
s0_snap_threshold = 50
s0_circle_color = #54befb80
s0_line_color = #1800ffff
s0_cross_line_color = #1800ffff

[screenshot]
s0_selection_outline_color = #2f2f4f9f
s0_selection_fill_color = #2f2f4f4f

[ezoom]
s0_zoom_box_outline_color = #2f2f4f9f
s0_zoom_box_fill_color = #2f2f2f4f

[animation]
s0_unminimize_effects = animation:Glide 2;

[core]
s0_active_plugins = core;composite;opengl;grid;compiztoolbox;snap;put;move;commands;mousepoll;regex;copytex;wall;resize;vpswitch;imgpng;animation;place;unitymtgrabhandles;workarounds;session;expo;fade;ezoom;scale;unityshell;
s0_outputs = 1920x1080+1920+69;1920x1080+0+0;
s0_autoraise = false
s0_autoraise_delay = 500
s0_raise_window_key = <Control><Alt>8
s0_show_desktop_key = <Control><Super>d
s0_hsize = 2
s0_vsize = 2

[shift]
s0_ground_color1 = #b3b3b3cc
s0_ground_color2 = #b3b3b300

[decor]
s0_active_shadow_color = #00000080
s0_inactive_shadow_color = #000000ff

[grid]
s0_put_center_key = Disabled
s0_put_left_key = <Control><Super>h
s0_put_right_key = <Control><Super>l
s0_put_top_key = <Control><Super>k
s0_put_bottom_key = <Control><Super>j
s0_put_topleft_key = <Control><Super>y
